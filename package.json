    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.30",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/anek-latin": "^4.5.6",
    "@fontsource/unbounded": "^4.5.1",
    "@storybook/addon-a11y": "^6.5.16",
    "sveltekit-i18n": "^2.2.2",
    "swiper": "^9.1.0"
